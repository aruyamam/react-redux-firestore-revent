{
   "extends": ["react-app", "airbnb"],
   "rules": {
      "indent": [
         "error",
         3,
         {
            "SwitchCase": 1
         }
      ],
      "brace-style": ["error", "stroustrup"],
      "react/jsx-indent": ["error", 3],
      "react/jsx-indent-props": ["error", 3],
      "no-shadow": "off"
   }
}
